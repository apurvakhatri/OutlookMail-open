{
  "name": "OutlookMail",
  "description": "Enjoy OutlookMail Integration on YellowAnt",
  "keywords": ["python3", "django2","yellowant"],
  "env": {
    "YA_DEVELOPER_TOKEN": {
      "description": "YellowAnt Developer Token",
      "required": true
    },
    "HEROKU_APP_NAME": {
      "description": "Heroku App name",
      "required": true
    },
    "OM_CLIENT_ID": {
      "description": "OutlookMail Client ID",
      "required": true
    },
    "OM_CLIENT_SECRET": {
      "description": "OutlookMail Client Secret",
      "required": true
    },
    "DISABLE_COLLECTSTATIC": {
      "required": false,
      "value": "1"
    },
    "ENV": {
      "required": true
      "value": "HEROKU"
    }

  }
}
